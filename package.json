{
  "name": "@slashnephy/eslint-config",
  "version": "3.0.8",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/eslint-config"
  },
  "license": "MIT",
  "author": "SlashNephy <spica@starry.blue> (https://spica.starry.blue/)",
  "type": "module",
  "main": "./src/index.js",
  "files": [
    "./package.json",
    "./src/**/*.js",
    "./src/**/*.ts",
    "./README.md"
  ],
  "scripts": {
    "build": "tsc",
    "bump-version": "tsx bin/bump-version.ts",
    "clean": "rm -rf src/**/*.js",
    "format": "eslint --fix",
    "lint": "eslint",
    "publish": "yarn build && yarn npm publish"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.4.1",
    "@eslint/js": "9.23.0",
    "@graphql-eslint/eslint-plugin": "4.4.0",
    "@next/eslint-plugin-next": "15.2.4",
    "@stylistic/eslint-plugin": "4.2.0",
    "@susisu/eslint-plugin-safe-typescript": "0.9.3",
    "@typescript-eslint/utils": "8.29.0",
    "@vitest/eslint-plugin": "1.1.38",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "4.3.1",
    "eslint-plugin-import-x": "4.10.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.16.2",
    "eslint-plugin-package-json": "0.29.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-relay": "1.8.3",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-tsdoc": "0.4.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-userscripts": "0.5.6",
    "eslint-plugin-xss": "0.1.12",
    "eslint-plugin-yml": "1.17.0",
    "globals": "16.0.0",
    "graphql": "16.10.0",
    "jsonc-eslint-parser": "2.4.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.0.2",
    "@types/eslint": "9.6.1",
    "@types/node": "22.13.17",
    "concurrently": "9.1.2",
    "eslint": "9.23.0",
    "jiti": "2.4.2",
    "tsx": "4.19.3"
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "packageManager": "yarn@4.8.1",
  "publishConfig": {
    "access": "public"
  }
}
